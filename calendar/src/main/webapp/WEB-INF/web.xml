<?xml version="1.0" encoding="utf-8" ?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
        <display-name>calendar project</display-name>
        <servlet>
            <servlet-name>events</servlet-name>
            <servlet-class>com.yang.event.EventsServlet</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>events</servlet-name>
            <url-pattern>/events</url-pattern>
        </servlet-mapping>
    <servlet>
        <servlet-name>stock</servlet-name>
        <servlet-class>com.yang.stock.StockServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>stock</servlet-name>
        <url-pattern>/stock</url-pattern>
    </servlet-mapping>
</web-app>